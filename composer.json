{
    "name": "articus/openapi-generator-common",
    "description": "Common classes for php-mezzio-ph and php-dt generators of OpenAPI Generator",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "articus/data-transfer": "^0.6"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/"
        }
    },
    "require-dev": {
        "kahlan/kahlan": "^5.2",
        "mockery/mockery": "^1.3",
        "symfony/polyfill-mbstring": "^1.27"
    }
}
